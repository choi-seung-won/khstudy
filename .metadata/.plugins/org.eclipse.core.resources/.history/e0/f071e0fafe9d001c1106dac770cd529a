
package day0307;

//사용자로부터 점수를 입력받아서

//A,B,C,D,F를 출력하는 프로그램.
//단 사용자가 잘못된 점수를 입력하면
//올바른 점수를 입력할 때까지 다시 입력을 받으세요
//7-15

import java.util.*;

public class Ex07GradeChecker {

    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        

        System.out.println("점수를 입력하시오");
        System.out.printf(">");
        while (scanner.nextInt() >= 0 || scanner.nextInt() <= 100) {

            scanner.nextInt();
            scanner.nextLine();

        }

        if (scanner.nextInt() >=90) {
            System.out.println("A");
        }else if(scanner.nextInt() >=80) {
            System.out.println("B");
        }else if(scanner.nextInt() >=70) {
            System.out.println("C");
        }else if(scanner.nextInt() >=60) {
            System.out.println("D");
        }else {
            System.out.println("F");
        }
    }

}
