package day0307;


import java.util.*;
public class Ex07GradeChecker {

    public static void main(String[] args) {
        
        Scanner scanner = new Scanner(System.in);
        
        System.out.println("점수를 입력하시오");
        
        int score = scanner.nextInt();
        
        if(score >= 90) {
            System.out.printf("A");
            
        }else if(score > 80) {
            System.out.printf("B");
        }else if(score >= 70) {
            System.out.printf("C");
        }else if(score >= 60) {
            System.out.printf("D");
        }else {
            System.out.printf("F");
        }
        System.out.println("학점입니다.");
        
        
        scanner.close();
    }

}
